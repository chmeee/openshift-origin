{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"_artifactsLocation": {
			"value": "https://raw.githubusercontent.com/chmeee/openshift-origin/release-3.7/"
		},
		"masterVmSize": {
			"value": "Standard_DS2_v2"
		},
		"infraVmSize": {
			"value": "Standard_DS2_v2"
		},
		"nodeVmSize": {
			"value": "Standard_DS2_v2"
		},
		"storageKind": {
			"value": "managed"
		},
		"openshiftClusterPrefix": {
			"value": "origin"
		},
		"masterInstanceCount": {
			"value": 1
		},
		"infraInstanceCount": {
			"value": 1
		},
		"nodeInstanceCount": {
			"value": NODECOUNT
		},
		"dataDiskSize": {
			"value": 128
		},
		"adminUsername": {
			"value": "origin"
		},
		"openshiftPassword": {
			"value": "PASSWORD"
		},
		"enableMetrics": {
			"value": "false"
		},
		"enableLogging": {
			"value": "false"
		},
		"enableCockpit": {
			"value": "false"
		},
		"sshPublicKey": {
			"value": "SSHPUBLIC_KEY"
		},
		"keyVaultResourceGroup": {
			"value": "KEYVAULT_RG"
		},
		"keyVaultName": {
			"value": "KEYVAULT_NAME"
		},
		"keyVaultSecret": {
			"value": "KEYVAULT_SECRET"
		},
		"enableAzure": {
			"value": "false"
		},
		"aadClientId": {
			"value": "AADCLIENT_ID"
		},
		"aadClientSecret": {
			"value": "AADCLIENT_SECRET"
		},
		"defaultSubDomainType": {
			"value": "nipio"
		},
		"defaultSubDomain": {
			"value": "changeme"
		}
	}
}
